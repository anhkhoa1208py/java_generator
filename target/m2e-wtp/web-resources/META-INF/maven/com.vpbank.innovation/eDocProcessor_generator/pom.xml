							<url>http://10.37.24.121:8130/HHOut?wsdl</url>
						</schema>
					</schemas>
				</configuration>
			</plugin> -->
		</plugins>
	</build>
	<repositories>
		<repository>
			<id>maven2_2</id>
			<url>https://repo1.maven.org/maven2/</url>
		</repository>
		<repository>
			<id>maven2_1</id>
			<url>https://repo.maven.apache.org/maven2/</url>
		</repository>
	</repositories>
</project>